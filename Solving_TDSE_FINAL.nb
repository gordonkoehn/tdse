Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"R0", "=", 
     RowBox[{"ConstantArray", "[", 
      RowBox[{"0", ",", 
       RowBox[{"Length", "[", "x", "]"}]}], "]"}]}], ";", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Initializing", " ", "wave", " ", "array", " ", "that", " ", "stores"}], 
     "*)"}], "\n", 
    RowBox[{"I0", "=", 
     RowBox[{"ConstantArray", "[", 
      RowBox[{"0", ",", 
       RowBox[{"Length", "[", "x", "]"}]}], "]"}]}], ";", "   ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "wave", " ", "values", " ", "at", " ", "each", " ", "gridpoint"}], " ", 
     "*)"}], "\n", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Calculate", " ", "the", " ", "initial", " ", "value", " ", "of", " ", 
      "each", " ", "gridpoint"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"For", "[", " ", 
     RowBox[{
      RowBox[{"nx", "=", "1"}], ",", " ", 
      RowBox[{"nx", "\[LessEqual]", "ngridpts"}], ",", " ", 
      RowBox[{"nx", "++"}], ",", "\[IndentingNewLine]", "\n", 
      RowBox[{
       RowBox[{
        RowBox[{"R0", "[", 
         RowBox[{"[", "nx", "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "0.5"}], "*", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"x", "[", 
                   RowBox[{"[", "nx", "]"}], "]"}], "-", 
                  RowBox[{"x", "[", 
                   RowBox[{"[", "wavecentreindex", "]"}], "]"}]}], ")"}], "/",
                 "s"}], ")"}], "^", "2"}]}], ")"}]}], ")"}], "*", 
         RowBox[{"Cos", "[", 
          RowBox[{"k", "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"x", "[", 
              RowBox[{"[", "nx", "]"}], "]"}], "-", 
             RowBox[{"x", "[", 
              RowBox[{"[", "wavecentreindex", "]"}], "]"}]}], ")"}]}], 
          "]"}]}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"I0", "[", 
         RowBox[{"[", "nx", "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "0.5"}], "*", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"x", "[", 
                   RowBox[{"[", "nx", "]"}], "]"}], "-", 
                  RowBox[{"x", "[", 
                   RowBox[{"[", "wavecentreindex", "]"}], "]"}]}], ")"}], "/",
                 "s"}], ")"}], "^", "2"}]}], ")"}]}], ")"}], "*", 
         RowBox[{"Sin", "[", 
          RowBox[{"k", "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"x", "[", 
              RowBox[{"[", "nx", "]"}], "]"}], "-", 
             RowBox[{"x", "[", 
              RowBox[{"[", "wavecentreindex", "]"}], "]"}]}], ")"}]}], 
          "]"}]}]}], ";"}]}], "\n", "]"}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Normalize", " ", "the", " ", "wavfunciton"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"modulus", " ", "=", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"R0", "^", "2"}], " ", "+", " ", 
            RowBox[{"I0", "^", "2"}]}], ")"}]}], "}"}], "]"}], ",", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{"modulus", "[", "xval", "]"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"xval", ",", " ", "0", ",", "L"}], "}"}]}], "]"}]}], " ", 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Saving", " ", "initial", " ", "wavefunction", " ", "values"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"R0", "=", 
     FractionBox["R0", 
      SqrtBox["A"]]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"I0", "=", 
     FractionBox["I0", 
      SqrtBox["A"]]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "check", " ", "if", " ", "normalization", " ", "was", " ", 
     "scuccesfull"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"modulusNormalized", " ", "=", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"R0", "^", "2"}], " ", "+", " ", 
            RowBox[{"I0", "^", "2"}]}], ")"}]}], "}"}], "]"}], ",", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"NIntegrate", "[", 
    RowBox[{
     RowBox[{"modulusNormalized", "[", "x", "]"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"x", ",", " ", "0", ",", "L"}], "}"}]}], "]"}], " ", 
   "\[IndentingNewLine]"}]}]], "Input",
