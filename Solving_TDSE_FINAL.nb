  RowBox[{
   RowBox[{"timeEvolution", "[", 
    RowBox[{"R0_", ",", " ", "I0_", " ", ",", " ", "V_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"times", ",", "RtFrames", ",", "ItFrames", ",", "PtFrames"}], 
      "}"}], ",", "\[IndentingNewLine]", 
